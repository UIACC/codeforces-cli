{
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "indent": 4,
    "latedef" : "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "quotmark" : true,
    "undef": true,
    "unused": true,
    "strict" : true,
    "trailing": true,
    "maxparams" : 3,
    "maxdepth" : 4,
    "maxlen" : 120,

    "eqnull": true,
    "globalstrict" : true,
    "laxcomma": true,

    "node" : true
}
